using System;

namespace uri1071
{
	class Program
	{
		static void Main(string[] args)
		{
			int n1, n2;
			int somar = 0;
		

			n1 = int.Parse(Console.ReadLine());
			n2 = int.Parse(Console.ReadLine());

			for (int i = n2 + 1; i < n1; i++)
			{
				
				if(i%2 != 0)
				{
					somar = somar + i;
					
				}
				
			}
			Console.WriteLine(somar);
			
		}
	}
}
